{
    "name": "tql",
    "displayName": "TypeQL Syntax",
    "description": "TypeQL Syntax Highlighter",
    "version": "0.2.2",
    "icon": "logo.png",
    "publisher": "idealley",
    "author": {
        "name": "Samuel Pouyt",
        "email": "samuelpouyt@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/idealley/graql-language-vscode"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "tql",
        "typeql",
        "typedb",
        "gql",
        "graql",
        "grakn"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tql",
                "aliases": [
                    "TypeDB",
                    "tql"
                ],
                "extensions": [
                    ".tql"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "gql",
                "aliases": [
                    "Graql",
                    "gql"
                ],
                "extensions": [
                    ".gql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tql",
                "scopeName": "source.tql",
                "path": "./syntaxes/tql.tmLanguage.json"
            },
            {
                "language": "gql",
                "scopeName": "source.gql",
                "path": "./syntaxes/gql.tmLanguage.json"
            }
        ]
    }
}