{
    "name": "typeql-plugin-vscode",
    "displayName": "TypeQL Syntax",
    "description": "TypeDB Syntax Highlighter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "typeql",
            "aliases": ["TypeQL", "typeql"],
            "extensions": [".tql",".gql"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "typeql",
            "scopeName": "source.tql",
            "path": "./syntaxes/tql.tmLanguage.json"
        }]
    }
}